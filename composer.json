{
  "name": "globalxtreme/response",
  "description": "GlobalXtreme Response Pattern",
  "authors": [
    {
      "name": "Yuswa",
      "email": "yuswa98@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "GlobalXtreme\\Response\\": "src/"
    },
    "files": [
      "src/Misc/helpers.php"
    ]
  },
  "require": {
    "php": "8.*",
    "globalxtreme/parser": "^1.0.0",
    "illuminate/support": "^8.83|^9.0"
  },
  "replace": {
    "globalxtreme/parser": "self.version"
  },
  "extra": {
    "laravel": {
      "providers": [
        "GlobalXtreme\\Response\\ResponseServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/globalxtreme/parser.git"
    }
  ]
}
